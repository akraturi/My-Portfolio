<section class="resume-section education-section mb-5">
  <h2 class="resume-section-title text-uppercase font-weight-bold pb-3 mb-3">Education</h2>
  <div class="resume-section-content">
    <ul class="list-unstyled">
      <% profile.education.forEach(function(education){ %>
        <li class="mb-2">
            <div class="resume-degree font-weight-bold"><%= education.title %></div>
            <div class="resume-degree-org"><%= education.institution %></div>
            <div class="resume-degree-time"><%= education.time %></div>
            <div class="d-inline-flex">
              <%- include('partials/snippets/edit.ejs',{form_url:"/education/"+education._id+"/edit",text:""}); %>
              <%- include('partials/snippets/remove.ejs',{delete_url:"/education/"+education._id+"/delete"}); %>
            </div>
        </li>
      <% }) %>
    </ul>
  </div>
    <%- include('partials/snippets/add.ejs',{add_url:'/education/new'});%>
</section><!--//education-section-->
